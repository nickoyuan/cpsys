
@model newtest.Models.dashboard_alertmodel

<table class="table table-hover no-margins" onload="myFunction()">
 
    <thead>
        <tr>
            <th>Site Name</th>
            <th>Date</th>
            <th>Alert</th>
        </tr>
    </thead>
    <tbody>
@for (var i = 0; i < Model.Alert.Count; i++)
{
    if (i < 8)
    {
        <tr>

          
            <td>@Html.DisplayFor(x => Model.Site[i])</td>

            <td><i class="fa fa-clock-o"></i>@Html.DisplayFor(x => Model.Date[i])</td>
            <td>@Html.DisplayFor(x => Model.Alert[i])</td>
           

        </tr>
    }
}
    </tbody>
 
</table>

<script>


    $(function myFunction() {

        $("#criticalviewcircle").remove();

    });

</script>

